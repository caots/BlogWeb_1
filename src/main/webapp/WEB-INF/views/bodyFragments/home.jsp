<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" %>
<main class="app-content">
    <div class="row">
        <div class="col-md-3">
            <div class="widget-small primary coloured-icon"><i class="icon fa fa-users fa-3x"></i>
                <div class="info">
                    <h4>User</h4>
                    <p><b id="user-number"></b></p>
                </div>
            </div>
        </div>
        <div class="col-md-3">
            <div class="widget-small info coloured-icon"><i class="icon fas fa-eye"></i>
                <div class="info">
                    <h4>View</h4>
                    <p><b id="view-number"></b></p>
                </div>
            </div>
        </div>
        <div class="col-md-3">
            <div class="widget-small warning coloured-icon"><i class="icon fab fa-elementor"></i>
                <div class="info">
                    <h4>Category</h4>
                    <p><b id="category-number"></b></p>
                </div>
            </div>
        </div>
        <div class="col-md-3">
            <div class="widget-small danger coloured-icon"><i class="icon far fa-newspaper fa-3x"></i>
                <div class="info">
                    <h4>News</h4>
                    <p><b id="news-number"></b></p>
                </div>
            </div>
        </div>
    </div>
</main>